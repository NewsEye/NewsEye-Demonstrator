#   git config --global core.excludesfile '~/.gitignore_global'

/.idea
/public/assets/*
/db/seeds_data
/db/old_seeds_data
/tools

# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
.env
/docker/docker_data_postgres
/docker/docker_data_fcrepo
/docker/docker_data_solr
/docker/log
/docker/dependencies

/lib/GeoLite2-City.mmdb

/vendor/cache
